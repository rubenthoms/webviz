{
    "name": "webviz",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "tsc && vite build",
        "webpack:dev": "NODE_ENV=development BABEL_ENV=development webpack serve --config webpack.config.cjs --no-open --port 3000 --hot --no-client-overlay",
        "webpack:build": "NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production BABEL_ENV=production webpack build --config webpack.config.cjs",
        "preview": "vite preview",
        "generate-api": "openapi --input http://127.0.0.1:8000/openapi.json --output ./src/api --client axios --name ApiService",
        "typecheck": "tsc --noEmit",
        "lint": "eslint 'src/**/*.+(ts|tsx|js|jsx|json)' 'tests/**/*.+(ts|tsx|js|jsx|json)'",
        "validate": "npm run typecheck && npm run lint",
        "test": "jest"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.8",
        "@heroicons/react": "^2.0.14",
        "@mui/base": "^5.0.0-alpha.116",
        "@tanstack/react-query": "^4.24.10",
        "@tanstack/react-query-devtools": "^4.24.12",
        "@webviz/subsurface-components": "~/webviz-subsurface-components-0.0.0.tgz",
        "axios": "^1.3.0",
        "lodash": "^4.17.21",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-plotly.js": "^2.6.0",
        "uniqolor": "^1.1.0",
        "uuid": "^9.0.0",
        "uuid4": "^2.0.3"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.19.1",
        "@babel/preset-react": "^7.18.6",
        "@svgr/webpack": "^6.5.1",
        "@trivago/prettier-plugin-sort-imports": "^4.0.0",
        "@types/jest": "^29.4.0",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.14.6",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.9",
        "@types/react-plotly.js": "^2.6.0",
        "@types/uuid": "^9.0.0",
        "@types/webpack": "^5.28.0",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "@vitejs/plugin-react": "^3.0.0",
        "autoprefixer": "^10.4.13",
        "babel-loader": "^9.1.2",
        "babel-preset-react-app": "^10.0.1",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-webpack-plugin": "^4.0.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^29.4.2",
        "mini-css-extract-plugin": "^2.7.3",
        "openapi-typescript-codegen": "^0.23.0",
        "postcss": "^8.4.21",
        "postcss-loader": "^7.0.2",
        "postcss-normalize": "^10.0.1",
        "postcss-preset-env": "^8.0.1",
        "prettier": "2.8.3",
